            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/backButton5"
            app:layout_constraintTop_toBottomOf="@+id/explainText4"
            app:layout_constraintVertical_bias="0.2"
            app:srcCompat="@drawable/next" />

        <ImageView
            android:id="@+id/prevImage2"
            android:layout_width="83dp"
            android:layout_height="83dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="116dp"
            app:layout_constraintBottom_toTopOf="@+id/explainText4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.708"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView12"
            app:layout_constraintVertical_bias="1.0"
            app:srcCompat="@drawable/iron_sulfide" />

        <TextView
            android:id="@+id/explainText4"
            android:layout_width="229dp"
            android:layout_height="236dp"
            android:fontFamily="@font/arang"
            android:text="\nIron sulfide is a diverse family of chemical compounds and minerals containing iron and sulfur, with a general formula of FeS.
            \n\n\Key examples include ferrous sulfide (FeS), pyrite (FeS2), and iron(II,III) sulfide (Fe3S4 or FeSÂ·Fe2S3). These compounds are often black, water-insoluble solids."
            android:textColor="#000000"
            android:textSize="12sp"
            app:layout_constraintBottom_toTopOf="@+id/imageView16"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/componentText5"
            android:layout_width="121dp"
            android:layout_height="39dp"
            android:layout_marginStart="88dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="24dp"
            android:fontFamily="@font/arang"
            android:gravity="center"
            android:text="Iron Sulfide"
            android:textColor="#000000"
            android:textSize="20sp"
            app:layout_constraintEnd_toStartOf="@+id/prevImage2"
            app:layout_constraintHorizontal_bias="0.75"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/outputText2" />

        <TextView
            android:id="@+id/mixText5"
            android:layout_width="119dp"
            android:layout_height="20dp"
            android:layout_marginStart="88dp"
            android:fontFamily="@font/arang"
            android:gravity="center"
            android:text="Sulfur + Iron"
            android:textColor="#000000"
            android:textSize="14sp"
            app:layout_constraintBottom_toTopOf="@+id/explainText4"
            app:layout_constraintEnd_toStartOf="@+id/prevImage2"
            app:layout_constraintHorizontal_bias="0.224"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/componentText5"
            app:layout_constraintVertical_bias="0.588" />

        <TextView
            android:id="@+id/outputText2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="108dp"
            android:fontFamily="@font/arang"
            android:text="Output"
            android:textColor="#000000"
            android:textSize="34sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView12" />

        <ImageButton
            android:id="@+id/backButton5"
            android:layout_width="53dp"
            android:layout_height="87dp"
            android:layout_marginBottom="136dp"
            android:backgroundTint="#00FFFFFF"
            android:scaleType="centerCrop"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.555"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/explainText4"
            app:layout_constraintVertical_bias="1.0"
